{
  "name": "prestashopcorp/grapesjs",
  "description": "Todo",
  "author": "PrestaShop",
  "config": {
    "preferred-install": "dist",
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "platform": {
      "php": "7.2.5"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "require": {
      "php": ">=7.2.5",
      "guzzlehttp/guzzle": "^7.5",
      "prestashop/prestashop-accounts-installer": "^1.0.1",
      "prestashop/module-lib-service-container": "^1.4",
      "segmentio/analytics-php": "^1.5",
      "scssphp/scssphp": "^1.11",
      "matthiasmullie/minify": "^1.3",
      "sentry/sentry": "^1.11.0",
      "symfony/string": "^5.4",
      "ramsey/uuid": "^4.2",
      "lcobucci/jwt": "^3.3"
  },
  "require-dev": {
      "prestashop/php-dev-tools": "^4.2",
      "phpunit/phpunit": "^8.5",
      "mikey179/vfsstream": "^1.6",
      "humbug/php-scoper": "0.13.9",
      "jetbrains/phpstorm-stubs": "dev-master"
  },
  "autoload": {
      "psr-4": {
        "PrestaShop\\Module\\GrapesJS\\": "src/",
        "PrestaShop\\Module\\Tests\\": "tests/",
        "PrestaShop\\Module\\GrapesJS\\Vendor\\ScssPhp\\ScssPhp\\": "vendor/scssphp/scssphp/src"
      },
    "classmap": [
      "grapesjs.php"
    ]
  },
  "type": "prestashop-module",
  "license": "AFL-3.0"
}
